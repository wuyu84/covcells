devtools::install_github('wuyu84/covcells')
library(covcells)

getwd()


cellmodels=readr::read_lines('/Volumes/sdata/0811起/元素模板库-职业版.txt')

save(cellmodels,file = './R/sysdata.rda')


data_test=readxl::read_xlsx('/Volumes/sdata/0811起/做图数据表/待做图.xlsx')
save(cellmodels,data_test,file = './R/sysdata.rda')
